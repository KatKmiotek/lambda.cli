{
  "compilerOptions": {
    "target": "ES2016",
    "strict": true,
    "preserveConstEnums": true,
    "noEmit": false,
    "module":"commonjs",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": true,
    "noImplicitAny": true
  },
  "exclude": ["node_modules", "**/*.test.ts"]
}
